function workAsDeveloper() {
  writeCode();
  runCode();
  seeBug();
  cry();
  fixBug();
  beHappyForTwoSeconds();
  seeAnotherBug();
}

function writeCode() {
    console.log("I'm writing code");
    startWriting();
    finishWriting();
}

function startWriting() {
  console.log("Okay, time to write something!");
}

function finishWriting() {
  console.log("Eh, not bad... Probably!");
}

function runCode() {
  console.log("I'm running my code");
}

function seeBug() {
  console.log('Gosh! A bug!');
}

function cry() {
    console.log("I'm the worst developer on Earth");
    moreCry();
}

function moreCry() {
    console.log("Snif snif");
}

function fixBug() {
  console.log("I'm the best developer on Earth");
}

function beHappyForTwoSeconds() {
  console.log('I should apply to work at Google');
}

function seeAnotherBug() {
  console.log('Oy, another bug!');
}

workAsDeveloper();
